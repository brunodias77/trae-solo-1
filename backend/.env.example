# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=BetsDB
DB_USER=postgres
DB_PASSWORD=your_postgres_password

# JWT Configuration
JWT_SECRET_KEY=your_jwt_secret_key_here_minimum_32_characters
